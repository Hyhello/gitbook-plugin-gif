{
	"name": "gitbook-plugin-gif",
	"version": "0.0.2",
	"description": "gitbook-plugin-gif",
	"main": "./dist/main.min.js",
	"files": [
		"src",
		"dist",
		"yarn.lock"
	],
	"scripts": {
		"lint": "eslint --ext .js",
		"lint:fix": "eslint --fix --ext .js",
		"build": "cross-env NODE_ENV=production rollup -c build/rollup.prod.js",
		"lint-staged": "pretty-quick --staged"
	},
	"keywords": [
		"gitbook",
		"plugin"
	],
	"author": "Hyhello",
	"license": "MIT",
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"lint-staged": {
		"**/*.js": [
			"eslint --ext .js",
			"prettier --write",
			"git add ."
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Hyhello/gitbook-plugin-gif.git"
	},
	"bugs": {
		"url": "https://github.com/Hyhello/gitbook-plugin-gif/issues"
	},
	"devDependencies": {
		"@commitlint/cli": "^8.0.0",
		"@commitlint/config-conventional": "^8.0.0",
		"autoprefixer": "^9.6.0",
		"cross-env": "^5.2.0",
		"eslint": "^5.16.0",
		"eslint-config-airbnb-base": "^13.1.0",
		"eslint-config-prettier": "^5.0.0",
		"eslint-friendly-formatter": "^4.0.1",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-plugin-import": "^2.17.3",
		"eslint-plugin-prettier": "^3.1.0",
		"husky": "^2.4.1",
		"jest": "^24.8.0",
		"lint-staged": "^8.2.1",
		"node-sass": "^4.12.0",
		"prettier": "^1.18.2",
		"pretty-quick": "^1.11.1"
	},
	"engines": {
		"node": ">=4.0.0",
		"gitbook": "*"
	},
	"browserslist": [
		"last 1 version",
		"> 1%",
		"maintained node versions",
		"not dead"
	]
}
